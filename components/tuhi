'use client';

export default function ActivityLogs() {
  const logs = [
    'Rahul logged in',
    'Ankit account locked',
    'Neha transferred funds',
  ];

  return (
    <div>
      <h1>Activity Logs</h1>
      <ul>
        {logs.map((log, i) => (
          <li key={i}>{log}</li>
        ))}
      </ul>
    </div>
  );
}